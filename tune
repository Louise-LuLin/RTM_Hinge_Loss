#!/bin/sh
nohup ./run -prefix ~/lab/dataset -source YelpNew -set byUser_20k_review -crossV 5 -nuOfTopics 5 -emIter 50 -topicmodel RTM > ./output/5_YelpNew_RTM_5.output 2>&1 &
nohup ./run -prefix ~/lab/dataset -source YelpNew -set byUser_20k_review -crossV 5 -nuOfTopics 10 -emIter 50 -topicmodel RTM > ./output/5_YelpNew_RTM_10.output 2>&1 &
